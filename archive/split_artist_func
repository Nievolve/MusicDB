from main_mdb import submappar
def split_artist(lista, tecken):
    artists = []
    for objekt in lista:
        delad_strang = ""
        for tecken_i_objekt in objekt:
            if tecken_i_objekt == tecken:
                break
            delad_strang += tecken_i_objekt
        artists.append(delad_strang)

    return artists

# Exempel
specifikt_tecken = "-"

resultat_lista = split_artist(submappar, specifikt_tecken)
print(resultat_lista)
