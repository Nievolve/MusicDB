import sqlite3
def create_database_and_table():
    conn = sqlite3.connect("test_musicdatabase.db")
    c = conn.cursor()
    c.execute("""CREATE TABLE IF NOT EXISTS tracks(
                id INTERGER PRIMARY KEY,
              title TEXT,
              album TEXT,
              artist TEXT,
              year INTERGER) 
    """)
    conn.commit()
    conn.close()
print("Staring")
create_database_and_table()
print("Ending")